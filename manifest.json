{
    "name": "Take note",
    "description": "You can quickly write note or record",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
          "storage"
        ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+W"
            }
        }
    }
}