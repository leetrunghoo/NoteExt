{
    "name": "Take note",
    "description": "Quickly take note and record voice note",
    "version": "1.2",
    "manifest_version": 2,
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "browser_action": {
        "default_icon": "img/icon128.png",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
          "storage"
        ],
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+W"
            }
        }
    }
}